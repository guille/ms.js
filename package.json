{
  "name": "ms",
  "version": "2.1.2",
  "repository": "zeit/ms",
  "description": "Tiny millisecond conversion utility",
  "unpkg": "dist/index.min.js",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "bench": "node bench",
    "build": "bundt index.js",
    "lint": "eslint *.js",
    "prebench": "npm run build",
    "precommit": "lint-staged",
    "pretest": "npm run build",
    "test": "mocha tests.js"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "benchmark": "2.1.4",
    "bundt": "0.4.0",
    "eslint": "4.12.1",
    "expect.js": "0.3.1",
    "husky": "0.14.3",
    "lint-staged": "5.0.0",
    "mocha": "4.0.1",
    "prettier": "1.17.1"
  }
}
